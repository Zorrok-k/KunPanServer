{
  "compilerOptions": {
    "target": "ES2020", 
    "esModuleInterop": true,
    "module": "commonjs", 
    "declaration": true, 
    "sourceMap": true, 
    "outDir": "./dist", 
    "rootDir": "./src", 
    // "removeComments": true, 
    "importHelpers": true, 
    "isolatedModules": true, 
    "incremental": true,
    "strict": true, 
    "noUnusedLocals": true, 
    "noUnusedParameters": true, 
    "noImplicitReturns": true, 
    "noFallthroughCasesInSwitch": true, 
  }
}
