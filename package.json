{
  "name": "kunpanserver",
  "version": "0.1",
  "main": "index.js",
  "bin": "src/index.ts",
  "pkg": {
    "targets": [
      "node23-win-x64"
    ],
    "assert": [
      "config.json",
      "public/**/*"
    ]
  },
  "scripts": {
    "dev": "set NODE_ENV=development && ts-node src/index.ts",
    "build": "pkg . --out-path=dist/",
    "start": "set NODE_ENV=production && node dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "_moduleAliases": {
    "@controllers": "./src/controllers",
    "@services": "./src/services",
    "@entities": "./src/entities",
    "@middlewares": "./src/middlewares",
    "@models": "./src/models",
    "@routes": "./src/routes"
  },
  "dependencies": {
    "better-sqlite3": "^11.10.0",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "nanoid": "^5.1.5",
    "typeorm": "^0.3.25"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.14",
    "module-alias": "^2.2.3",
    "tslib": "^2.8.1",
    "winston": "^3.17.0"
  }
}
